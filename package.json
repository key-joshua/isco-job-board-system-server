{
	"version": "1.0.0",
	"main": "src/index.ts",
	"name": "isco-job-board-system-server",
	"description": "This is the server for the ISCO Job Board System, which is a platform that connects job seekers with employers. It provides functionalities such as job posting and applications.",
	"scripts": {
		"deleteAllTables": "tsc && npx sequelize db:migrate:undo:all",
		"deleteAllSeeds": "tsc && npx sequelize db:seed:undo:all",
		"createAllTables": "tsc && npx sequelize db:migrate",
		"createAllSeeds": "tsc && npx sequelize db:seed:all",
		"kill-window": "Taskkill /IM node.exe /F",
		"kill-linux": "killall -9 node",
		"start": "ts-node ./src/index",
		"dev": "nodemon ./src/index",
		"build": "tsc --build",
		"lint": "eslint src"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/key-joshua/isco-job-board-system-server.git"
	},
	"keywords": [
		"ISCO",
		"JOB",
		"BOARD"
	],
	"author": "Josue",
	"license": "ISC",
	"homepage": "https://github.com/key-joshua/isco-job-board-system-server#readme",
	"dependencies": {
		"bcrypt": "^5.1.1",
		"cloudinary": "^2.7.0",
		"connect-multiparty": "^2.2.0",
		"cors": "^2.8.5",
		"dotenv": "^16.0.3",
		"express": "^4.17.1",
		"express-status-monitor": "^1.2.3",
		"http-status-codes": "^2.3.0",
		"joi": "^17.13.3",
		"jsonwebtoken": "^9.0.2",
		"nodemailer": "^6.9.14",
		"nodemon": "^3.1.4",
		"pg": "^8.12.0",
		"pg-hstore": "^2.3.4",
		"randomstring": "^1.3.0",
		"sequelize": "^6.37.7",
		"sequelize-cli": "^6.6.3",
		"sequelize-typescript": "^2.1.6",
		"ts-node": "^10.9.2",
		"typescript": "^5.4.5",
		"typescript-eslint": "^7.3.1"
	},
	"devDependencies": {
		"@babel/core": "^7.21.3",
		"@types/bcrypt": "^5.0.2",
		"@types/cors": "^2.8.13",
		"@types/express": "^4.17.17",
		"@types/express-status-monitor": "^1.3.3",
		"@types/jsonwebtoken": "^9.0.6",
		"@types/node": "^13.13.52"
	}
}
